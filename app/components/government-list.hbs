{{#if this.governments.length}}
  {{#each this.governments key="id" as |government|}}
    <GovernmentCard @government="{{government}}"></GovernmentCard>
  {{/each}}
{{else}}
  <BsAlert
          @visible={{true}}
          @fade={{true}}
          @type="info"
          @dismissible={{true}}
  >
    Geen vlaamse regering teruggevonden.
  </BsAlert>
{{/if}}



